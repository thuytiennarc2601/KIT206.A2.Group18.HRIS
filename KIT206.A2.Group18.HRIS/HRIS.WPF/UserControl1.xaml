<UserControl x:Class="HRIS.WPF.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HRIS.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="0,0,10,0">
        <TextBox x:Name="staffIDTextBox" HorizontalAlignment="Left" Height="50" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="192"/>
        <TextBox x:Name="startTextBox" HorizontalAlignment="Left" Height="58" Margin="9,114,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="198"/>
        <TextBox x:Name="endTextBox" HorizontalAlignment="Left" Height="44" Margin="9,172,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="184"/>
        <ComboBox x:Name="dayComboBox" HorizontalAlignment="Left" Height="44" Margin="9,70,0,0" VerticalAlignment="Top" Width="246"
                  ItemsSource="{Binding Source={StaticResource dayValues}}"/>

    </Grid>
</UserControl>
